# Archestra platform connection
ARCHESTRA_API_URL=http://localhost:9000
ARCHESTRA_API_KEY=archestra_your_api_key_here

# Transport: "stdio" (inside Archestra K8s) or "sse" (standalone HTTP mode)
TRANSPORT=stdio

# Only needed if TRANSPORT=sse
PORT=8080
CORS_ORIGIN=*

# Logging: DEBUG | INFO | WARN | ERROR
LOG_LEVEL=INFO

# LLM model for deep scans and testing (e.g. gpt-5-mini, google/gemini-2.0-flash-exp:free)
LLM_MODEL=gpt-5-mini
